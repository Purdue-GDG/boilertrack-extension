{
    
    "manifest_version": 3,
    "name": "Boilertrack",
    "version": "1.0.0",
    "icons": {
        "16": "images/gdglogo16.png",
        "32": "images/gdglogo32.png",
        "48": "images/gdglogo48.png",
        "128": "images/gdglogo128.png"
      },

      "background": {
        "service_worker": "service-worker.js",
        "type": "module"
      },
      "action": { },
      "permissions": [
        "scripting"
      ],
      "host_permissions": [
        "http://*/*", 
        "https://*/*"
      ]

}